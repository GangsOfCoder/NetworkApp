<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/tools"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:clickable="true"
    app:ignore="NamespaceTypo"
    card_view:cardCornerRadius="5dp"
    card_view:cardElevation="2dp"
    card_view:cardUseCompatPadding="true"
    card_view:rippleColor="#cfd8dc">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/_70sdp"
        android:background="#FAFAFA"
        android:orientation="vertical">


        <TextView
            android:id="@+id/textView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="Person 01"
            android:textSize="@dimen/_12ssp"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="@dimen/_20sdp"
            android:layout_height="@dimen/_20sdp"
            android:layout_marginTop="8dp"
            android:background="@drawable/ic_call"
            android:backgroundTint="@color/colorPrimary"
            card_view:layout_constraintStart_toStartOf="@+id/textView6"
            card_view:layout_constraintTop_toBottomOf="@+id/textView6" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="Call"
            android:textColor="@color/colorBlackGray"
            card_view:layout_constraintBottom_toBottomOf="@+id/imageView11"
            card_view:layout_constraintStart_toEndOf="@+id/imageView11"
            card_view:layout_constraintTop_toTopOf="@+id/imageView11" />

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="@dimen/_16sdp"
            android:background="@drawable/ic_access_time"
            android:backgroundTint="@color/colorPrimary"
            card_view:layout_constraintBottom_toBottomOf="@+id/textView21"
            card_view:layout_constraintStart_toEndOf="@+id/textView21"
            card_view:layout_constraintTop_toTopOf="@+id/textView21" />

        <TextView
            android:id="@+id/textView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="10:35 AM"
            android:textColor="@color/colorBlackGray"
            card_view:layout_constraintBottom_toBottomOf="@+id/imageView13"
            card_view:layout_constraintStart_toEndOf="@+id/imageView13"
            card_view:layout_constraintTop_toTopOf="@+id/imageView13" />
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>